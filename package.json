{
  "name": "vue-use-load-deps-monorepo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev:core": "pnpm --filter vue-lazy-deps-loader dev",
    "dev:demo": "pnpm --filter vue-lazy-deps-loader-demo dev",
    "dev": "concurrently \"pnpm dev:core\" \"pnpm dev:demo\"",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.0.0"
  }
} 